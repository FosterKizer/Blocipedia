<div class="row">
  <div class="col s8 offset-s2 center-align">
    
    <br /><h4>My wikis</h4><br />
    
    <% @user.wikis.each do |wiki| %>
      <p><%= link_to wiki.title, wiki_path(wiki.id) %></p>
    <% end %>
    
    <%= link_to "New Wiki", new_user_wiki_path(@user, @wiki), class: 'btn grey darken-1' %>
    
  </div>
</div>
